[options]
library = "Gegl"
version = "0.4"
min_cfg_version = "0.4"
target_path = "."
girs_directories = ["../gir-files/", "../"]
work_mode = "normal"
single_version_file = true
generate_safety_asserts = true
deprecate_by_min_version = true

generate = [
	"Gegl.*",
	"Gegl.AbyssPolicy",
	"Gegl.AccessMode",
	"Gegl.BablVariant",
	"Gegl.BlitFlags",
	"Gegl.CachePolicy",
	"Gegl.Config",
	"Gegl.DistanceMetric",
	"Gegl.DitherMethod",
	"Gegl.LookupFunction",
	"Gegl.MapFlags",
	"Gegl.MetadataHash",
	"Gegl.NodeFunction",
	"Gegl.Operation",
	"Gegl.Orientation",
	"Gegl.PadType",
	"Gegl.Random",
	"Gegl.RectangleAlignment",
	"Gegl.ResolutionUnit",
	"Gegl.SamplerGetFun",
	"Gegl.SamplerType",
	"Gegl.SerializeFlag",
	"Gegl.SplitStrategy",
	"Gegl.Stats",
	"Gegl.TileCommand",
	"Gegl.TileSource",
	"Gegl.TileSourceCommand",
	"Gegl.MetadataStore",
	"Gegl.AudioFragment",
	"Gegl.Buffer",
	"Gegl.TileHandler",
	# "Gegl.TileBackend",
	# "Gegl.Tile",
]

[[object]]
name = "Gegl.Rectangle"
status = "generate"
boxed_inline = true
	[[object.function]]
	name = "infinite_plane"
	ignore = true

[[object]]
name = 'Gegl.Tile' 
status = "ignore" # a record

[[object]]
generate_builder = true
name = 'Gegl.TileBackend' 
status = "generate"
	[[object.function]]
	name = "get_extent"
	ignore = true

	[[object.function]]
	name = "get_format"
	ignore = true

manual = []
